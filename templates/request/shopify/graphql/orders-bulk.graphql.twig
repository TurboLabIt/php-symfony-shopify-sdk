{# https://shopify.dev/api/usage/bulk-operations/queries #}
mutation {
    bulkOperationRunQuery(
        query:"""
        {% include 'request/shopify/graphql/orders.graphql.twig' %}
        """
    ) {
        bulkOperation {
            id
            status
        }
        userErrors {
            field
            message
        }
    }
}
